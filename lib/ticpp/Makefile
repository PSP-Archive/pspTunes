ifndef config
  config=debug
endif
export config

PROJECTS := TiCPP

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

TiCPP: 
	@echo ==== Building TiCPP ====
	@${MAKE} --no-print-directory -C . -f TiCPP.make

clean:
	@${MAKE} --no-print-directory -C . -f TiCPP.make clean

help:
